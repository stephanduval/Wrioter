\par\pard\plain \tx0\tx360\tx720\tx1080\tx1440\tx1800\tx2160\tx2880\tx3600\tx4320\li763\fi-475\sa240\ql\ltrch\loch {\f0\fs24 <$Scr_Ps::0>}{\f0\fs24 1.\tab }{\field{\*\fldinst HYPERLINK "scrivlnk://0E55C8CA-8028-4B08-AEBB-32D5D19BC24D"}{\fldrslt\f0\fs24 Compile}}{\f0\fs24  the Draft folder into a single document for printing or exporting.}
